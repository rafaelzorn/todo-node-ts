{
  "name": "todo-node-ts",
  "version": "1.0.0",
  "description": "A Simple TODO api",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "start:dev": "ts-node-dev --poll --respawn --transpile-only -r tsconfig-paths/register src/server.ts",
    "test": "jest --passWithNoTests --runInBand",
    "test:watch": "jest --passWithNoTests --watchAll --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafaelzorn/todo-node-ts.git"
  },
  "keywords": [
    "node",
    "typescript",
    "express"
  ],
  "author": "Rafael Zorn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rafaelzorn/todo-node-ts/issues"
  },
  "homepage": "https://github.com/rafaelzorn/todo-node-ts/blob/master/README.md",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.3",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
